    <script type="text/javascript">
        //<![CDATA[
        $(document).ready(function () {
            // 맞춤 컨텐츠 팝업 노출 - - - - - - - - - - - -
            var userType = getCookie("AMUSERTYPE");
            if (userType == null || userType == "N") {
                $(".modal").removeClass("hide");
                GA_Virtual("알바몬>메인>회원유형선택", "/main/virtual/layerUserType");
            }
            else {
                $(".modal").addClass("hide");
            }

            $("#btn_MType").on("click", function () {
                mainmethod.setUserType("M", true);
            });

            $("#btn_CType").on("click", function () {
                mainmethod.setUserType("C", true);
            });
            // - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
        });
    </script>
