                    <script type="text/javascript">

                        $(document).ready(function () {
                            var dev_bizmon_prd_layer_cookie = '';

                            dev_bizmon_prd_layer_cookie = $('#dev_bizmon_prd button.btnbzprdclose').data('id');

                            if (dev_bizmon_prd_layer_cookie == undefined || dev_bizmon_prd_layer_cookie == '') {
                                dev_bizmon_prd_layer_cookie = 'IsPrimiumLayer';
                            }


                            if (getCookie(dev_bizmon_prd_layer_cookie) != "Y") {
                                $("#dev_bizmon_prd .banner").show(); //상품 있을 경우
                            }

                            $("#btn_goBizmon2016GiftEvent").on("click", function () {
                                $.ajax({
                                    url: "/Api/BizEvt-Click-Update?v_clickType=1&v_position=3",
                                    type: "post",
                                    dataType: 'json',
                                    cache: false,
                                    async: false,
                                    complete: function (result) {
                                        var win = window.open('https://bizmon.albamon.com/partner/services/noticeview?Notice_No=56');
                                        win.focus();
                                    }
                                });
                            });
                        });
                    </script>

                    <script type="text/javascript">
                        BizmonPrdShow();
                        function BizmonPrdShow() {
                            aryBizmonPrd.sort(function () { return Math.random() * 2 - 1; });
                            var $bizul = $("#dev_bizmon_prd .base");
                            var bizPrd = "";
                            var BizmonPrdFormat = [];
                            BizmonPrdFormat.push("<li>");
                            BizmonPrdFormat.push("  <a href='javascript:void(0)' class='item' onclick=\"mainlayer.openInNewTab({1}); GA_Event('홈_PC', '비즈몬프리미엄비즈 포스트', '{6}_{7}');\">");
                            BizmonPrdFormat.push("      <span class='thumbnail'>");
                            BizmonPrdFormat.push("      <img src='{2}' alt=''onerror='{5}' > </span>");
                            BizmonPrdFormat.push("      <span class='name'>{3}</span>");
                            BizmonPrdFormat.push("      <span class='title'>{4}</span>");
                            BizmonPrdFormat.push(" </a>");
                            BizmonPrdFormat.push("</li>");


                            var divideNumB = 5;
                            var TotalBizCnt = aryBizmonPrd.length;
                            var TotalLoopBG = 0;

                            if (TotalBizCnt % divideNumB > 0)
                                TotalLoopBG = (parseInt((TotalBizCnt / divideNumB)) * divideNumB) + divideNumB;

                            if (aryBizmonPrd.length > 0) {
                                for (var i = 0; i < aryBizmonPrd.length; i++) {
                                    bizPrd = BizmonPrdFormat.join("");
                                    bizPrd = bizPrd.replace("{0}", aryBizmonPrd[i].postUrl);
                                    bizPrd = bizPrd.replace("{1}", aryBizmonPrd[i].postno);
                                    bizPrd = bizPrd.replace("{2}", aryBizmonPrd[i].imageUrl);
                                    bizPrd = bizPrd.replace("{3}", aryBizmonPrd[i].coname);
                                    bizPrd = bizPrd.replace("{4}", aryBizmonPrd[i].title);
                                    bizPrd = bizPrd.replace("{5}", "this.src=\"https://contents.albamon.kr/monimg/bizmon/empty_bizmon.png\"");
                                    bizPrd = bizPrd.replace("{6}", aryBizmonPrd[i].coname);
                                    bizPrd = bizPrd.replace("{7}", aryBizmonPrd[i].title);
                                    var $bizPrd = $(bizPrd);

                                    $bizPrd.appendTo($bizul);
                                }

                                for (var i = 0; i < (TotalLoopBG - TotalBizCnt); i++) {
                                    var $li = $("<li></li>");
                                    $li.html("<a href='https://bizmon.albamon.com/partner/post/post_reg' class='rent' target='_blank'><img src='https://contents.albamon.kr/monimg/main/bizmon/rent.png?v=20201014' alt='이 곳에 귀사의 비즈니스를 홍보해보세요!'><span class='title'>등록하러 가기</span></a>");
                                    $li.appendTo($bizul);
                                }

                            } else {
                                for (var i = 0; i < divideNumB; i++) {
                                    var $li = $("<li></li>");
                                    $li.html("<a href='https://bizmon.albamon.com/partner/post/post_reg' class='rent' target='_blank'><img src='https://contents.albamon.kr/monimg/main/bizmon/rent.png?v=20201014' alt='이 곳에 귀사의 비즈니스를 홍보해보세요!'><span class='title'>등록하러 가기</span></a>");
                                    $li.appendTo($bizul);
                                }
                            }

                        }
                    </script>




