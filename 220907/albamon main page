        window.dsHelper = {
            registVal: function (key, value) {
                window.dsValDic[key] = value;
            },
            getVal: function (key) {
                return window.dsValDic[key];
            },
            removeVal: function (key) {
                delete (window.dsValDic[key]);
            },
