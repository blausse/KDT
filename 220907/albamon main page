                    <script type="text/javascript">
                        SpecialDraw();

                        function SpecialDraw() {

                            arySpecial_F.sort(function () { return Math.random() * 2 - 1; });
                            arySpecial_G.sort(function () { return Math.random() * 2 - 1; });
                            arySpecial.sort(function () { return Math.random() * 2 - 1; });
                            arySpecial_JK.sort(function () { return Math.random() * 2 - 1; });

                            var SpecialFormat = [];
                            SpecialFormat.push("<tr {0}>");
                            SpecialFormat.push("   <td class='area'>{1}</td>");
                            SpecialFormat.push("    <td class='cmp'>{5}</td>");
                            SpecialFormat.push("   <td class='sub'>");
                            SpecialFormat.push("       <span class='sWrap'>");
                            SpecialFormat.push("            {2}"); // img
                            SpecialFormat.push("            {3}"); // a
                            SpecialFormat.push("            {4}"); // button
                            SpecialFormat.push("    </td>");
                            SpecialFormat.push("    <td class='date'>{7}</td>");
                            SpecialFormat.push("    <td class='pay'>{6}</td>");
                            SpecialFormat.push("</tr>");

                            var SpecialF = "";
                            var SpecialG = "";
                            var Special = "";
                            var SpecialJK = "";
                            var SpecialB = "";

                            for (var i = 0; i < arySpecial_F.length; i++) {

                                SpecialF = SpecialFormat.join("");
                                SpecialF = SpecialF.replace("{0}", "class=''");
                                SpecialF = SpecialF.replace("{1}", arySpecial_F[i].dutyGuCode);
                                SpecialF = SpecialF.replace("{2}", "");

                                linkType = 0;
                                SpecialF = SpecialF.replace("{3}", arySpecial_F[i].strSubjectClass);
                                SpecialF = SpecialF.replace("{4}", "<button type='button' class='pgSimpleGI smr' id='pgSimpleSpePL_" + arySpecial_F[i].alGINo + "' optgcd='" + arySpecial_F[i].clickCount + "'><span>요약</span></button>");
                                SpecialF = SpecialF.replace("{5}", "<a href='#' onclick=\"GIB_Read(" + arySpecial_F[i].alGINo + "," + linkType + ",'" + arySpecial_F[i].clickCount + "'); return false;\" >" + arySpecial_F[i].c_name + "</a>");

                                if (arySpecial_F[i].isEduTrainee.toLowerCase() == "true") {
                                    SpecialF = SpecialF.replace("{6}", "<span class='eduTrainee'>교육 · 연수생</span>");
                                } else {
                                    SpecialF = SpecialF.replace("{6}", arySpecial_F[i].strPay);
                                }
                                SpecialF = SpecialF.replace("{7}", arySpecial_F[i].strworkingTime);

                                var $SpecialF = $(SpecialF);
                                $SpecialF.appendTo("#special");

                            }

                            for (var i = 0; i < arySpecial_G.length; i++) {
                                SpecialG = SpecialFormat.join("");
                                SpecialG = SpecialG.replace("{0}", "class='gold'");
                                SpecialG = SpecialG.replace("{1}", arySpecial_G[i].dutyGuCode);
                                SpecialG = SpecialG.replace("{2}", arySpecial_G[i].opt_Icon);
                                SpecialG = SpecialG.replace("{3}", arySpecial_G[i].strSubjectClass);
                                SpecialG = SpecialG.replace("{4}", "<button type='button' class='pgSimpleGI smr' id='pgSimpleSpePL_" + arySpecial_G[i].alGINo + "' optgcd='" + arySpecial_G[i].clickCount + "'><span>요약</span></button>");
                                SpecialG = SpecialG.replace("{5}", arySpecial_G[i].strLink + arySpecial_G[i].c_name + "</a>");

                                if (arySpecial_G[i].isEduTrainee.toLowerCase() == "true") {
                                    SpecialG = SpecialG.replace("{6}", "<span class='eduTrainee'>교육 · 연수생</span>");
                                } else {
                                    SpecialG = SpecialG.replace("{6}", arySpecial_G[i].strPay);
                                }
                                SpecialG = SpecialG.replace("{7}", arySpecial_G[i].strworkingTime);

                                var $SpecialG = $(SpecialG);

                                $SpecialG.appendTo("#special");
                            }

                            for (var i = 0; i < arySpecial.length; i++) {
                                Special = SpecialFormat.join("");
                                Special = Special.replace("{0}", "");
                                Special = Special.replace("{1}", arySpecial[i].dutyGuCode);
                                Special = Special.replace("{2}", arySpecial[i].opt_Icon);
                                Special = Special.replace("{3}", arySpecial[i].strSubjectClass);
                                Special = Special.replace("{4}", "<button type='button' class='pgSimpleGI smr' id='pgSimpleSpePL_" + arySpecial[i].alGINo + "' optgcd='" + arySpecial[i].clickCount + "'><span>요약</span></button>");
                                Special = Special.replace("{5}", arySpecial[i].strLink + arySpecial[i].c_name + "</a>");

                                if (arySpecial[i].isEduTrainee.toLowerCase() == "true") {
                                    Special = Special.replace("{6}", "<span class='eduTrainee'>교육 · 연수생</span>");
                                } else {
                                    Special = Special.replace("{6}", arySpecial[i].strPay);
                                }
                                Special = Special.replace("{7}", arySpecial[i].strworkingTime);

                                var $Special = $(Special);

                                $Special.appendTo("#special");
                            }

                            DrawSpecialBanner(SpecialFormat);


                            for (var i = 0; i < arySpecial_JK.length; i++) {

                                SpecialJK = SpecialFormat.join("");
                                SpecialJK = SpecialJK.replace("{0}", "");

                                aLink = "<a href='" + arySpecial_JK[i].linkUrl + "' target='_blank'>";
                                SpecialJK = SpecialJK.replace("{1}", arySpecial_JK[i].dutyGuCode);
                                SpecialJK = SpecialJK.replace("{2}", "");
                                SpecialJK = SpecialJK.replace("{3}", aLink + arySpecial_JK[i].subject);
                                SpecialJK = SpecialJK.replace("{4}", aLink + "<button type='button' class='smrjk'><span>새창</span></button></a>");
                                SpecialJK = SpecialJK.replace("{5}", aLink + arySpecial_JK[i].c_name);

                                if (arySpecial_JK[i].isEduTrainee.toLowerCase() == "true") {
                                    SpecialJK = SpecialJK.replace("{6}", "<span class='eduTrainee'>교육 · 연수생</span>");
                                } else {
                                    SpecialJK = SpecialJK.replace("{6}", arySpecial_JK[i].strPay);
                                }
                                SpecialJK = SpecialJK.replace("{7}", arySpecial_JK[i].strworkingTime);

                                var $SpecialJK = $(SpecialJK);

                                $SpecialJK.appendTo("#special");
                            }
                        }

                        function DrawSpecialBanner(SpecialFormat) {
                            arySpecial_B.sort(function () { return Math.random() * 2 - 1; });
                            for (var i = 0; i < arySpecial_B.length; i++) {

                                SpecialB = SpecialFormat.join("");
                                SpecialB = SpecialB.replace("{0}", "");
                                SpecialB = SpecialB.replace("{1}", arySpecial_B[i].dutyGuCode);
                                SpecialB = SpecialB.replace("{2}", arySpecial_B[i].opt_Icon);
                                SpecialB = SpecialB.replace("{3}", arySpecial_B[i].strSubjectClass);
                                SpecialB = SpecialB.replace("{4}", "<button type='button' class='pgSimpleGI smr' id='pgSimpleSpePL_" + arySpecial_B[i].alGINo + "' optgcd='" + arySpecial_B[i].clickCount + "'><span>요약</span></button>");
                                SpecialB = SpecialB.replace("{5}", arySpecial_B[i].strLink + arySpecial_B[i].c_name + "</a>");

                                if (arySpecial_B[i].isEduTrainee.toLowerCase() == "true") {
                                    SpecialB = SpecialB.replace("{6}", "<span class='eduTrainee'>교육 · 연수생</span>");
                                } else {
                                    SpecialB = SpecialB.replace("{6}", arySpecial_B[i].strPay);
                                }
                                SpecialB = SpecialB.replace("{7}", arySpecial_B[i].strworkingTime);

                                var $SpecialB = $(SpecialB);

                                $SpecialB.appendTo("#special");


                            }
                        }

                        $(function () {
                            var int_spedefaults = 0;
                            $("#dev_MainSpecial tbody tr").not(".gold").each(function () {
                                int_spedefaults++;
                                if (int_spedefaults > 0 && int_spedefaults % 5 == 0) {
                                    $(this).addClass('dLine');
                                }
                            });

                        });
                    </script>


                </div>

                <!--// 전문 채용관 -->
                <script type="text/javascript">
                    function banner_click01(optgf, url, newflag) {
                        document.getElementById("albamontargetURL").contentWindow.location.href = "/list/gi/banner_click_count.asp?optgf=" + optgf;
                        if (newflag == "1") {
                            window2 = window.open(url);
                            window2.focus();
                        }
                        else {
                            top.location.href = url;
                        }
                    }
                </script>


                <!-- Point알바 -->

                <script type="text/javascript">
                    var aryPoint = new Array();
                </script>



                <script type="text/javascript">

                    PointDraw();
                    function PointDraw() {

                        var TotalCnt = aryPoint.length;
                        var divideNumB = 2;
                        var TotalLoopB = 0;

                        if (TotalCnt % divideNumB > 0)
                            TotalLoopB = ((TotalCnt / divideNumB) * divideNumB) + divideNumB;
                        else
                            TotalLoopB = TotalCnt;

                        aryPoint.sort(function () { return Math.random() * 2 - 1; });

                        var PointFormat = [];
                        PointFormat.push("   <td {6}>{1}</td>");
                        PointFormat.push("   <td {7}>");
                        PointFormat.push("      <span class='sWrap'>");
                        PointFormat.push("         {2}");
                        PointFormat.push("         {3}");
                        PointFormat.push("         {4}");
                        PointFormat.push("         ");
                        PointFormat.push("   </td>");
                        PointFormat.push("   <td {8}>{5}</td>");


                        var Point = "";
                        var $tr = $("<tr></tr>");
                        var $Point = "";

                        if (TotalCnt > 0) {
                            for (var i = 0; i < TotalLoopB; i++) {

                                if (i < aryPoint.length) {
                                    Point = PointFormat.join("");
                                    Point = Point.replace("{1}", aryPoint[i].dutyGuCode);
                                    Point = Point.replace("{2}", aryPoint[i].opt_Icon);
                                    Point = Point.replace("{3}", aryPoint[i].strSubjectClass);
                                    Point = Point.replace("{4}", "<button type='button' class='pgSimpleGI smr' id='pgSimplePoiPL_" + aryPoint[i].alGINo + "' optgcd='" + aryPoint[i].clickCount + "'><span>요약</span></button>");



                                    if (aryPoint[i].bgColor == "2") {
                                        Point = Point.replace("{5}", (aryPoint[i].strLink).replace("<a", "<a class='cmp'") + aryPoint[i].c_name + "</a>");
                                        Point = Point.replace("{6}", "class='pbg'");
                                        Point = Point.replace("{7}", "class='pbg'");
                                        Point = Point.replace("{8}", "class='pbg'");

                                    }
                                    else {
                                        Point = Point.replace("{5}", aryPoint[i].strLink + aryPoint[i].c_name + "</a>");
                                        Point = Point.replace("{6}", "");
                                        Point = Point.replace("{7}", "");
                                        Point = Point.replace("{8}", "");
                                    }


                                    $Point = $(Point);
                                    $Point.appendTo($tr);



                                }
                                else {

                                    $Point = $("<td colspan='3'  class='none'></td>")
                                    $Point.html("<a href='#' onclick=\"javascript:goMainLoc('/RecruitManager/Recruit/Regist-Form?PRDTCode=Point&amp;utm_source=blank_banner&amp;utm_medium=click&amp;utm_campaign=point','','');return false;\"><img src='https://contents.albamon.kr/monimg/main/rent_point_1.gif'  alt='부담없는 비용으로 알바몬 메인광고의 효과를 확인하세요! 상품안내 및 신청' /></a>");
                                    $Point.appendTo($tr);
                                }



                                if (i > 0 && (i + 1) % 2 == 0) {
                                    $tr.appendTo("#Point");
                                    $tr = $("<tr></tr>");
                                }

                            }
                        }
                        else {
                            $tr.html(" <td colspan='3' class='none'><a href='#' onclick=\"javascript:goMainLoc('/RecruitManager/Recruit/Regist-Form?PRDTCode=Point&amp;utm_source=blank_banner&amp;utm_medium=click&amp;utm_campaign=point','','');return false;\"><img src='/monimg/main/rent_point_1.gif'  alt='부담없는 비용으로 알바몬 메인광고의 효과를 확인하세요! 상품안내 및 신청' /></a></td><td colspan='3' class='none'><a href='#' onclick=\"javascript:goMainLoc('/RecruitManager/Recruit/Regist-Form?PRDTCode=Point&amp;utm_source=blank_banner&amp;utm_medium=click&amp;utm_campaign=point','','');return false;\"><img src='/monimg/main/rent_point_1.gif'  alt='부담없는 비용으로 알바몬 메인광고의 효과를 확인하세요! 상품안내 및 신청' /></a></td>");
                            $tr.appendTo("#Point");
                        }
                    }

                    $(function () {
                        $('#dev_MainPoint tr:nth-child(5n)').addClass('dLine');
                        $('#dev_MainPoint td:nth-child(2)').addClass('sub');
                        $('#dev_MainPoint td:nth-child(3)').addClass('line');
                    });


                </script>






