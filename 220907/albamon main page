                    <script type="text/javascript">
                        var aryNotice = new Array();
                        var ShowFlag = "1"; //개인
                        $.ajax({
                            url: "/Main/Login/getMemTypeCode",
                            type: "GET",
                            async: false,
                            success: function (result, textStatus) {
                                if (result) {
                                    if ($.trim(result) == "C") {
                                        ShowFlag = "2"; //기업
                                    }
                                    else if ($.trim(result) == "N") {
                                        ShowFlag = "0";
                                    }
                                }
                            },
                            error: function (textStatus) {
                                ShowFlag = "0";
                            }
                        });

                    </script>
                    <script type="text/javascript">

                        obj =
