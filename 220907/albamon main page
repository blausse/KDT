            <script type="text/javascript">
                var gnbFixedStat = "0";
            </script>
            <script type="text/javascript">
                var gnbiPv2 = Math.ceil(Math.random() * $('#dev_gnbNotice2 li').length - 1);
                var gnbFixedStat = "0";
                gnbiBinit(gnbiPv2, 'dev_gnbNotice2');

                //-- GNB fixed Set - - - -
                $(window).load(function () {
                    if ($.trim(gnbFixedStat) == "1") {
                        // 새로고침 했을 경우를 대비한 메소드 실행
                        scrollController();

                        // 스크롤을 하는 경우에만 실행됨
                        $(window).scroll(function () {
                            scrollController();
                        });

                        // 메뉴 가릴경우 뿔 아이콘 제거
                        $(window).resize(function () {
                            if ($("div.secNavWrap").hasClass("secNavWrap_1")) {
                                if ($(window).width() <= 1050) {
                                    $('.secNavWrap .albamonIcon').removeClass('on');
                                } else {
                                    $('.secNavWrap .albamonIcon').addClass('on');
                                }
                            }
                        });
                    }
                });

                // 메인 메뉴의 위치를 제어하는 함수
                function scrollController() {
                    currentScrollTop = $(window).scrollTop();
                    if (currentScrollTop < 130) {
                        $("div.secNavWrap").removeClass("secNavWrap_1");
                        $('.secNavWrap .albamonIcon').removeClass('on');
                    } else {
                        $("div.secNavWrap").addClass("secNavWrap_1");
                        if ($(window).width() > 1000) {
                            $('.secNavWrap .albamonIcon').addClass('on');
                        }
                    }
                }
            </script>
        </div>


        <div id="container" class="">







            <div id="subContentbg">




                <div class="mPoint">
                    <div id="contentStart" class="skip">본문 시작</div>
                    <h2 class="skip">알바몬 전체 컨텐츠</h2>
                    <div class="pointSide">

                        <!--메인 로그인-->
                        <div class="mainLogin"></div>
                        <script type="text/javascript">
                            $.ajax({
                                url: "/Main/Login/Setting/",
                                async: false,
                                type: "post",
                                dataType: "jsonp",
                                success: function (d, s) {
                                    if (d.Info != undefined && d.Info != null) {

                                        var $a = $(d.Info);
                                        $a.appendTo(".mainLogin");
                                    }
                                }
                            });
                        </script>
                        <!--  메인 로그인 하단배너(VIP배너)  -->
                        <div class="bnrSide" id="dev_vip_bnnr"></div>
                        <script type="text/javascript">
                            var arySbanner1 = new Array();
                        </script>
                        <script type="text/javascript">
                            DrawSbanner();
                            function DrawSbanner() {
                                var $alink = "";
                                if (arySbanner1 != null && arySbanner1.length > 0) {
                                    arySbanner1.sort(function () { return Math.random() * 2 - 1; });
                                    $alink = $(arySbanner1[0].StrHtml);
                                }
                                else {
                                    var randomImg = [], targetImg, targetImgNo;
                                    randomImg[0] = 'https://img.albamon.kr/banner/2022/01/faityz10094927.jpg';
                                    randomImg[1] = 'https://img.albamon.kr/banner/2022/01/jsakqq10094936.jpg';

                                    targetImgNo = Math.floor(Math.random() * randomImg.length);
                                    targetImg = randomImg[targetImgNo];

                                    $alink = $("<span id='dev_bnnr1'><a href='/community/Event/Event_list' target='_blank'><img src='" + targetImg + "' alt='랭키닷컴 아르바이트 사이트 순위 1위' width='240' height='145' /></a></span>");
                                }

                                $alink.appendTo(".bnrSide");
                            }

                            function set_count(alink, idx) {
                                $.ajax({
                                    url: "/Main/Etc/sbanner_view_cnt?idx=" + idx,
                                    type: "GET",
                                    error: function (info, xhr) {
                                        location.href = alink.href;
                                    },
                                    success: function (req) {
                                    }
                                });
                            }

                        </script>



