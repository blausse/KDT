                    <script type="text/javascript">
                        DrawNotice();
                        function DrawNotice() {
                            if (ShowFlag == "2") {
                                $(".shortCutWrap .personal").addClass("skip");
                                $(".shortCutWrap .corp").removeClass("skip");
                            }
                            else if (ShowFlag == "1") {
                                $("#devContract").attr("href", "/mem/mon_contract_mng.asp");
                            }

                            aryNotice.sort(function () { return Math.random() * 2 - 1; });
                            var Notice = "";
                            var NoticeFormat = [];
                            NoticeFormat.push("<dl reg_group='{1}'>");
                            NoticeFormat.push("    <dt><a href='{2}' target='_top' onclick=\"GA_Event('홈_PC', '{6}', '');\">{3}</a></dt>");
                            NoticeFormat.push("    <dd><a href='{4}' target='_top' onclick=\"GA_Event('홈_PC', '{7}', '');\">{5}</a></dd>");
                            NoticeFormat.push(" </dl>");

                            if (aryNotice != null && aryNotice.length > 0) {
                                if (aryNotice.length > 2)
                                    aryNotice.length = 2;

                                for (var i = 0; i < aryNotice.length; i++) {
                                    Notice = NoticeFormat.join("");
                                    Notice = Notice.replace("{1}", aryNotice[i].reg_group);
                                    if (aryNotice[i].b_type == "2") {
                                        Notice = Notice.replace("{3}", "이벤트");
                                        Notice = Notice.replace("{6}", "이벤트");
                                        Notice = Notice.replace("{7}", "이벤트");
                                    } else {
                                        Notice = Notice.replace("{3}", "공지");
                                        Notice = Notice.replace("{6}", "공지사항");
                                        Notice = Notice.replace("{7}", "공지사항");
                                    }
                                    Notice = Notice.replace("{2}", "/service/company/notice_read.asp?B_No=" + aryNotice[i].b_no);
                                    Notice = Notice.replace("{4}", "/service/company/notice_read.asp?B_No=" + aryNotice[i].b_no);
                                    Notice = Notice.replace("{5}", aryNotice[i].sub_title);
                                    var $Notice = $(Notice);
                                    $Notice.appendTo(".newList");
                                }
                            }
                        }
                    </script>




