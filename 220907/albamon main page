                                $(document).ready(function () {
                                    $('#search_favorkeyList > li > a').click(function (e) {
                                        var favorkey = $(this).text();
                                        try {
                                            GA_Event('공통_PC', '검색', '인기_' + favorkey);
                                        } catch (e) {
                                            console.error(e);
                                        }
                                        location.href = "/search?Keyword=" + encodeURIComponent(favorkey, true);
                                        return false;
                                    });
