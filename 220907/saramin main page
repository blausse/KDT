    <script>var category = 'main_curtain_banner',
            curtainBanner = document.querySelector(".wrap_curtain_banner"),
            main = document.getElementById('sri_main'); main.classList.add('be_curtain_banner'); function closeCurtainBanner(notThirdView) {
                // 3일동안 보지 않기
                if (notThirdView) {
                    setCookie(category, 'disable', 3);
                }  // 배너 닫힘 처리
                curtainBanner.style.display = 'none';
                document.querySelector(".wing").style.top = 366 + 'px';
                main.classList.remove('be_curtain_banner');
            } function setCookie(name, value, expiredays) {
                var todayDate = new Date();
                todayDate.setDate(todayDate.getDate() + expiredays);
                document.cookie = name + "=" + escape(value) + "; path=/; expires=" + todayDate.toGMTString() + ";"
            }</script>
